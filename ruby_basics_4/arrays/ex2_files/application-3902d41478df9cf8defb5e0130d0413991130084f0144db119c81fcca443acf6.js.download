  var $modal = $("#feedback_widget_modal"),
      $form = $("form", $modal),
      $editor = $(".editor-with-markdown-preview", $modal),
      $outputElement = $(".markdown-preview", $editor),
      $textarea = $("textarea", $editor),
      $actions = $(".actions", $editor);

  function runFeedbackMarkdownPreview(e) {
    $textarea.hide();
    $actions.hide();
    $outputElement.show();
