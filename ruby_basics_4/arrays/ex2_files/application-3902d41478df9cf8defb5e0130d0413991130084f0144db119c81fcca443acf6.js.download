// Some of the old courses are stored in the database and include sublime video
// specific markup. This code modifies their markup to be compatible with the
// newer videojs player.
//
// This file needs to run BEFORE the code in track_activities.js.

$(function() {

  $('video.sublime').each(function(i, video) {
    var $video = $(video),
        $videoWrapper;


    $video.removeClass('sublime')
      .addClass('video-js vjs-launch-school-skin vjs-16-9')
      .attr('poster', '/assets/tealeaf_markup/video_poster.jpg')
      .attr('controls', true)

    if (!video.id) {
      $video.attr('id', 'video_' + i);
    }

    $('source', video).attr('type', 'video/mp4');

    initializeVideoForElement(video.id)

    $videoWrapper = $video.parent();

    removeOldDownloadLink($videoWrapper);
    addNewDownloadLink($videoWrapper);

    // Make the video responsive
    removeDimensionsFromWrapper($videoWrapper);
  });

  function removeDimensionsFromWrapper($wrapper) {
    $wrapper.css({
      height: 'auto',
      width:  'auto'
    });
  }

  function removeOldDownloadLink($el) {
    // The old html contains a p followed by a ul which contain the label and
    // link to download the video.
    //
    // Note from Brandon Conway:
    // I also found old html without the following ul and the p has different
    // text. To handle that, I added the `else if` clause.
    var $elementAfterVideo = $el.next(),
        $elementAfterNext  = $elementAfterVideo.next();

    if ($elementAfterVideo.is('p:contains("Download:")') && $elementAfterNext.is('ul')) {
      $elementAfterVideo.remove();
      $elementAfterNext.remove();
    } else if ($elementAfterVideo.is('p:contains("Download Video")')) {
      $elementAfterVideo.remove();
    }
  }

  function addNewDownloadLink($el) {
    var url = $('source', $el).attr('src'),
        $meta = $('<div class="video-meta clearfix">\
                     <p class="download-video-links" style="padding: 1em;">\
                       <a href="' + url + '" rel="download-hd-video">\
                         <span class="fa fa-arrow-circle-o-down"></span> Download HD Video\
                       </a>\
                     </p>\
                   </div>');

    $meta.insertAfter($el);
  }
});
// This file adds event listeners for two video activities we track: playback
// and download.
$(function() {

  function trackActivity(verb) {
    var url_array = document.URL.split('/'),
        assignment_token = url_array[url_array.length - 1];

    $.ajax({
      // full URL is `/lessons/:lesson_id/assignments/:assignment_id/activities/track_activity`
      // we are using relative URL here
      url: assignment_token + "/activities/track_activity",
      type: "POST",
      data: {verb: verb}
    });
  }

  // "firstplay" only fires the first time a video is played, but not
  // subsequent plays (such as after a pause.)
  $('.wrapper').on('firstplay', 'section#assignment .video-js', function() {
    trackActivity("play_video");
  });

  $("section#assignment .download-video-links a").click(function(event) {
    trackActivity("download_video");
  });

  // The next 2 event handlers are for the older courses with hard-coded video
  // tags and download links.
  $("section#assignment a:contains('Video')").click(function(event) {
    trackActivity("download_video");
  });

  $("section#assignment a:contains('Download')").click(function(event) {
    trackActivity("download_video");
  });
});

