document.addEventListener("turbolinks:load", function() {
  $(".quiz-hide-all-answers").click(function() {
    $(".question-item").addClass("review-display");
  });
  $(".toggle-quiz-question-review-display").click(function() {
    $(this)
      .closest(".question-item")
      .toggleClass("review-display");
  });
});
document.addEventListener('turbolinks:load', function () {
  var container = document.querySelector('.track-radios');

  if (container) {
    container.querySelectorAll('[type=radio]').forEach(function (radio) {
      radio.addEventListener('change', function () {
        if (this.checked) {
          container.setAttribute('data-active-track', this.value);
        }
      });
    });
  }
});
$(function() {
  function filterVideos(text) {
    var $videos = $('article');
    var matcher = new RegExp(text, 'i');

    if (text === '') {
      $videos.show(0);
    } else {
      toggleVideos($videos, matcher);
    }
  }

  function toggleVideos($videos, matcher) {
    $videos.each(function(){
      var $video = $(this);
      var videoContent = $video.data('content');

      if (matcher.test(videoContent)) {
        $video.show(0);
      } else {
        $video.hide(0);
      }
    });
  }
  
  $('#video-filter-value').on('keyup', function() {
    filterVideos($(this).val());
  })
});
$(document).on('turbolinks:load', function() {
  const $filters = $('#completed-assessments-filters select');
  const assessments = document.querySelectorAll('#completed-assessments tbody tr');
  const $graderFilter = $('select#grader');
  const $courseFilter = $('select#course');
  const $assessmentTypeFilter = $('select#assessment_type');
  const $verdictFilter = $('select#verdict');
  const $technicalFilter = $('select#technical');
  const $nonTechnicalFilter = $('select#non_technical');

  function showAll() {
    assessments.forEach(assessment => assessment.style.display = 'table-row')
  }

  function removeNotSelectedGraders() {
    [...$graderFilter.find('option:not(:selected)')]
      .map(item => item.textContent)
      .forEach(grader => {
        [...document.querySelectorAll(`[data-grader="${grader}"`)].forEach(grader => grader.style.display = 'none');
    });
  }

  function removeNotSelectedCourses() {
    [...$courseFilter.find('option:not(:selected)')]
      .map(item => item.textContent)
      .forEach(course => {
        [...document.querySelectorAll(`[data-course="${course}"`)].forEach(course => course.style.display = 'none');
    });
  }

  function removeNotSelectedAssessmentType() {
    [...$assessmentTypeFilter.find('option:not(:selected)')]
      .map(item => item.textContent)
      .forEach(assessmentType => {
        [...document.querySelectorAll(`[data-assessment-type="${assessmentType}"`)].forEach(assessmentType => assessmentType.style.display = 'none');
    });
  }

  function removeNotSelectedVerdict() {
    [...$verdictFilter.find('option:not(:selected)')]
      .map(item => item.textContent)
      .forEach(verdict => {
        [...document.querySelectorAll(`[data-verdict="${verdict}"`)].forEach(verdict => verdict.style.display = 'none');
    });
  }

  function removeNotSelectedTechnical() {
    [...$technicalFilter.find('option:not(:selected)')]
      .map(item => item.textContent)
      .forEach(technical => {
        [...document.querySelectorAll(`[data-technical="${technical}"`)].forEach(technical => technical.style.display = 'none');
    });
  }

  function removeNotSelectedNonTechnical() {
    [...$nonTechnicalFilter.find('option:not(:selected)')]
      .map(item => item.textContent)
      .forEach(nonTechnical => {
        [...document.querySelectorAll(`[data-non-technical="${nonTechnical}"`)].forEach(nonTechnical => nonTechnical.style.display = 'none');
    });
  }

  function updateMatchingAssessments() {
    const totalAssessments = assessments.length;
    const filteredAssessments = document.querySelectorAll('#completed-assessments tbody tr[style="display: none;"]').length;
    const displayedAssessments = totalAssessments - filteredAssessments;
    const assessmentCountDisplay = document.querySelector('#assessments-count');

    if (displayedAssessments > 1 || displayedAssessments === 0) {
      assessmentCountDisplay.textContent = `${displayedAssessments} matching assessments`;
    } else {
      assessmentCountDisplay.textContent = `${displayedAssessments} matching assessment`;
    }
  }

  $filters.change(function() {
    showAll();
    removeNotSelectedGraders();
    removeNotSelectedCourses();
    removeNotSelectedAssessmentType();
    removeNotSelectedVerdict();
    removeNotSelectedTechnical();
    removeNotSelectedNonTechnical();
    updateMatchingAssessments();
  });
});
