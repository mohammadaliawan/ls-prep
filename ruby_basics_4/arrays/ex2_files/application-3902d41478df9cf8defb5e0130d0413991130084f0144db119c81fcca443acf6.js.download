// modernizr is required for foundation

// Turbolinks need to trigger window.onload on page:load for masonry and other use cases.
// Otherwise it will be triggered by default on initial page load. We need to highlight
// here, because prism.js default relies on page initially loaded and highlights only in
// that case. We need to manually highlight now.
document.addEventListener("page:load", function() {
  $(window).trigger("load");
  Prism.highlightAll();
});

$(function() {
  $(".tablesorter").tablesorter();
});

// Foundation only provides the close-reveal-modal class
// which has css which moves it to the upper right corner
// of the modal. This allows us to use .close-modal-inline
// to create a custom modal close button.
//
// NOTE: does not work in FireFox, Safari in local dev
$(document).on("click", ".close-modal-inline", function(e) {
  var $modal = $(e.target).closest(".reveal-modal");
  $modal.foundation("reveal", "close");
  return false;
});

$(function() {
  $(document).foundation();

  // This allows us to add a "full-height" class to any
  // foundation column and it will be set to the same height
  // as its parent row. This is useful when a vertical border
  // needs to be applied to the entire row height. This was
  // added specifically so the course description on the
  // course show page can be the full row height.
  $('.full-height').each(function() {
    var $this = $(this);

    $this.height($this.parent().height());
  });

  setFullWidthBackgroundElementsHeight();
});

$(window).on('resize', setFullWidthBackgroundElementsHeight);

// This sets the minimum height of the last non-header and non-footer element
// which have a full width color background. When these elements do not have
// enough children to fill the page til the bottom of the browser, there is a
// section that is the wrong color. This fixes that issue, and is a bandaid
// until I can find an all css solution.
function setFullWidthBackgroundElementsHeight() {
  var $el = $('body > .content .full-width-background, #main-content .full-width-background').last();

  if ($el.length) {
    var distanceFromTop = $el.offset().top;

    $el.css({
      minHeight: $('body').height() - distanceFromTop
    });
  }
}

// Window.onload shim for turbolinks
//
// Window.onload will be triggered on the first page load as normal, and then
// manually for Turbolinks pages.
// ref: https://github.com/turbolinks/turbolinks-classic/issues/295#issuecomment-70036212
window.loadEventsBound = false;
$(document).on('page:change', function(){
  if(window.loadEventsBound) {
    // Assuming all JS and CSS is downloaded and processed on the first page load...
    // ...then when loading a new page via Turbolinks the only thing delaying Window.onload is Images.
    // So use: http://imagesloaded.desandro.com -- and JavaScript is all like "You images done yet or what?"
    imagesLoaded('body', function(){
      $(window).trigger('load');
    });
  }
});

$(window).on('load', function(){
  window.loadEventsBound = true;
});
// End Window.onload shim

$(document).on('change', '#success-stories [name="story_type"]', function(e) {
  var $selector = $('.story-type-selector');
  var $selected = $('#success-stories [name="story_type"]:checked')

  $('.active', $selector).each(function() {
    $(this).removeClass('active');
  })

  $('[for="' + $selected[0].id + '"]', $selector).addClass('active');
});

$(window).load(function() {
  $('.masonry-container').masonry({
    itemSelector: '.masonry-item',
    columnWidth: '.grid-sizer',
    percentPosition: true,
    gutter: 15,
  });
});

// We have an unknown csrf token problem, this is a supposed solution
// ref: https://github.com/rails/jquery-ujs/issues/456#issuecomment-183728902
$(document).on('page:load', function(){
  $.rails.refreshCSRFTokens();
});

