  $assessmentThread.on("blur", ".award-points input", function(e) {
    var $target       = $(e.currentTarget),
        $awardPoints  = $target.parent(),
        score         = $target.val(),
        max           = parseInt($target.attr("max"), 10),
        scoreAsNumber = parseInt(score, 10),
        errorMessage;

    if (scoreAsNumber.toString() !== score) {
      errorMessage = "The input must be an integer. The input " + score + " is not an Integer.";
    } else if (scoreAsNumber < 0) {
      errorMessage = "Negative value is invalid.";
    } else if (scoreAsNumber > max) {
      errorMessage = "Value above max score is invalid.";
    }

    function addToolTip($input, message) {
      $input.addClass("has-tip");
      $input.attr("data-tooltip", true);
      $input.attr("aria-haspopup", true);
      Foundation.libs.tooltip.getTip($input).html(message + "<span class='nub'></span>");
      $input.attr("title", message);
      $(document).foundation("tooltip");
    };

    function removeToolTip($input) {
      $input.removeClass("has-tip");
      $input.removeAttr("data-tooltip");
      $input.removeAttr("aria-haspopup");
      $input.removeAttr("title");
    };

    if (errorMessage) {
      $awardPoints.addClass("error");
      addToolTip($target, errorMessage);
