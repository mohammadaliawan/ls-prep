$(document).on('blur', '.validate-email input[type="email"]', function() {
  $validated_div = $(this).closest('.validate-email');

  $(this).mailcheck({
    suggested: function(element, suggestion) {
      $suggestion_div = $('<div class="email-suggestion">Did you mean: <a class="suggestion" href="javascript:void(0);">' + suggestion.full + '</a>?</div>');
      if ($validated_div.find('.email-suggestion').length > 0) {
        $validated_div.find('.email-suggestion').replaceWith($suggestion_div);
      } else {
        $validated_div.append($suggestion_div);
      }
    },
    empty: function(element) {
      if ($validated_div.find('.email-suggestion').length > 0) {
        $validated_div.find('.email-suggestion').remove();
      }
    }
  });
});

$(document).on('click', '.validate-email .email-suggestion .suggestion', function() {
  $validated_div = $(this).closest('.validate-email');

  $validated_div.find('input[type="email"]').val($(this).text());

  if ($validated_div.find('.email-suggestion').length > 0) {
    $validated_div.find('.email-suggestion').remove();
  }
});
$(document).ready(function() {
  $('body').removeClass('preload');

  $('pre').each(function (index, pre) {
    if(!$(pre).is('[class*="language-"]')) {
      if ($(pre).attr("lang") == "irb") {
        $(pre).addClass('language-irb');
      } else if ($(pre).attr("lang") == "bash") {
        $(pre).addClass('language-bash');
      } else {
        $(pre).addClass('language-ruby');
      }
    }
  });

  // fixes prism, tealeaf markup and replaces language-none with language-ruby
  // check docs/readme_for_books/book_migration_doc.md for more info
  $(".bookshelf pre > code.language-none").each(function (index, code) {
    $(code).attr('class', 'language-ruby');
  });

  $('li.chapter_title > a.chapter_title').on('click', function(e) {
    e.preventDefault();
    $(this).parent().next().slideToggle();
    $(this).parent().toggleClass('open');
  });

  $('.solution h4').on('click', function(e) {
    e.preventDefault();
    $(this).next().slideToggle();
    $(this).parent().toggleClass('open');
  });

  $('.video_walkthrough h4').on('click', function(e) {
    var $this = $(this);
    $(this).parent().toggleClass('open');
    $(this).next().slideToggle();

    if ($this.parent().hasClass('open')) {
      $(document).trigger('resize_image_maps');
    }

    e.preventDefault();
  });
});
(function() {
  $(function() {
    var sideMenu;
    sideMenu = {
      $el: $('nav.side-menu'),
      $overlay: $('.close-side-menu'),
      close: function(e) {
        this.$el.addClass('hide');
        return this.$overlay.addClass('hide');
      },
      selectLink: function(e) {
        var hashtag, href;
        href = $(e.currentTarget).attr('href');
        hashtag = href.match(/#.*$/)[0];
        if (hashtag) {
          this.scrollToSection(hashtag);
          return false;
        }
      },
      scrollToSection: function(hashtag) {
        var anchorLocation;
        hashtag = hashtag.replace('#', '');
        anchorLocation = $('a[name="' + hashtag + '"]').offset().top;
        this.close();
        if (anchorLocation) {
          return $('html, body').scrollTop(anchorLocation - 100);
        }
      },
      adjustOnPageLoad: function() {
        var hashtag;
        if (this.$el.is(':visible')) {
          hashtag = window.location.hash;
          if (hashtag) {
            return this.scrollToSection(hashtag);
          }
        }
      },
      open: function(e) {
        this.$el.removeClass('hide');
        return this.$overlay.removeClass('hide');
      },
      bind: function() {
        this.bindCloseButton();
        this.bindCloseOverlay();
        this.bindLinks();
        this.bindToggleButton();
        return this.bindPageLoad();
      },
      bindCloseButton: function() {
        var closeButton;
        closeButton = this.$el.find('button.close');
        return closeButton.on('click', $.proxy(this.close, this));
      },
      bindCloseOverlay: function() {
        var closeButton;
        closeButton = $('.close-side-menu');
        return closeButton.on('click', $.proxy(this.close, this));
      },
      bindLinks: function() {
        var menu;
        menu = this;
        return this.$el.find('a:not([class*="chapter_title"])').each(function() {
          return $(this).on('click', $.proxy(menu.selectLink, menu));
        });
      },
      bindToggleButton: function() {
        return $('[data-toggle-side-menu]').on('click', $.proxy(this.open, this));
      },
      bindPageLoad: function(e) {
        return $(window).on('load', $.proxy(this.adjustOnPageLoad, this));
      },
      init: function() {
        return this.bind();
      }
    };
    return sideMenu.init();
  });

}).call(this);
