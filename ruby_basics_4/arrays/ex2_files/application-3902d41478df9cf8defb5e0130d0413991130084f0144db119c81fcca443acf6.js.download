$(document).ready(function() {
  $('#forum-actions .search-form').submit(function(e) {
    var $form  = $(e.target),
        $input = $('#discussion-search-term', $form);

    if ($input.val().trim() === '') {
      $input.toggleClass('invalid', true);

      if ($input.next('small.error').length === 0) {
        $input.after(
          $('<small>', {
            class: 'error',
            text: 'Please include a word or phrase to search for.'
          })
        );
      }
      e.preventDefault();
    } else {
      $input.toggleClass('invalid', false);

      if ($input.next('small.error').length > 0) {
        $input.next('small.error').remove();
      }
    }
  });


  $('#forum-actions .search-form').on('focus', '#discussion-search-term', function(e) {
    var $input = $(e.currentTarget),
        $form = $(e.currentTarget).closest('form'),
        $submit = $('input[type="submit"]', $form);

    $input.removeClass('empty');
    $submit.removeClass('hide');
  });

  $('#forum-actions .search-form').on('blur', '#discussion-search-term', function(e) {
    var $input = $(e.currentTarget),
        $form = $(e.currentTarget).closest('form'),
        $submit = $('input[type="submit"]', $form),
        inputEmpty = $input.val().trim() === '';

    $input.toggleClass('empty', inputEmpty);
    $submit.toggleClass('hide', inputEmpty);
  });

  $('#forum-actions select[name="categories"]').on('change', function(e) {
    var selectedCategory = $(this).val();

    if (selectedCategory) {
      window.location = window.location.pathname + '?category=' + selectedCategory;
    } else {
      window.location = window.location.pathname
    }
  });

  $('form.edit_post').submit(function() {
    var $submitButton = $(this).find('[type="submit"]'),
        buttonText = $submitButton.text().trim();

    if (buttonText === "Post") {
      $submitButton.text('Posting...');
    } else if (buttonText === "Update") {
      $submitButton.text('Updating...');
    }
    $submitButton.attr('disabled', 'disabled');
  });

  $('form.new_comment, form.comment_on_solution').submit(function() {
    var $submitButton = $(this).find('[type="submit"]');
    $submitButton.text('Posting...');
    $submitButton.attr('disabled', 'disabled');
  });

  $('form.edit_comment, form.edit_solution').submit(function() {
    var $submitButton = $(this).find('[type="submit"]');
    $submitButton.text('Updating...');
    $submitButton.attr('disabled', 'disabled');
  });
});
