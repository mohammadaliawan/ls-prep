$(function() {
  var $projectBucket = $("#project-bucket");
  if ($projectBucket.length === 0) { return; };

  var $fileInput    = $(".file-input", $projectBucket),
      $form         = $fileInput.closest("form"),
      s3PostURL     = $form.data("s3-post-url"),
      projectToken  = $form.data("project-token"),
      $submitButton = $("input[type=submit]", $form),
      $buttonTitle  = $(".file-upload-button-title", $form),
      $currentFile  = $(".current-file", $form),
      $hiddenFileUrl       = $("#student-file-url", $form),
      $hiddenFileName      = $("#student-file-name", $form),
      $previewSubmitButton = $(".preview-submit", $projectBucket),
      $projectQuestionContent = $(".project-question-content", $projectBucket),
      errorMessageBase = "<i class='fa fa-exclamation-triangle red' aria-hidden='true'></i>";

  function showSpinner() {
    $buttonTitle.html("<i class='fa fa-spinner fa-spin' aria-hidden='true'></i> Uploading ...");
  };

  function hideSpinner() {
    $buttonTitle.html("Select a File ...");
  };

  function validateProjectSteps() {
    var checkBoxes = $projectQuestionContent.find("input.ls-checkbox").toArray(),
        $fileUploadButton = $(".file-upload");

    if (checkBoxes.length === 0) {
      $fileUploadButton.removeAttr("disabled");
      $fileInput.removeAttr("disabled");
      $submitButton.removeAttr("disabled");
      return;
    };

    function isChecked(element, index, array) {
      return element.checked;
    };

    if (checkBoxes.every(isChecked)) {
      $fileUploadButton.removeAttr("disabled");
      $fileInput.removeAttr("disabled");
    } else {
      $fileUploadButton.attr("disabled", true);
      $fileInput.attr("disabled", true);
    };
    return;
  };

  function disableAllCheckBoxes() {
    var $checkBoxes = $projectQuestionContent.find("input.ls-checkbox");
    if ($checkBoxes.length === 0) { return; }

    if ($(".disable-checkboxes", $projectBucket).length !== 0) {
      $checkBoxes.attr("checked", "checked");
      $checkBoxes.attr("disabled", "disabled");
    };
  };

  $("#project-bucket").on("click", ".show-project-question", function(e) {
    var $this       = $(e.currentTarget),
        $hideButton = $(".hide-project-question");

    $this.hide();
    $hideButton.show();
    $projectQuestionContent.show("slow");
    e.preventDefault();
  });

  $("#project-bucket").on("click", ".hide-project-question", function(e) {
    var $this       = $(e.currentTarget),
        $showButton = $(".show-project-question");

    $this.hide();
    $showButton.show();
    $projectQuestionContent.hide("slow");
    e.preventDefault();
  });

  $currentFile.on("click", ".remove-current-file", function(e) {
    e.preventDefault();
    $hiddenFileUrl.val("");
    $hiddenFileName.val("");
    $fileInput.val("");
    $currentFile.empty();
    if ($submitButton.val() === "Resubmit Project") {
      $submitButton.val("Post Comment");
      $previewSubmitButton.text("Post Comment");
    } else {
      $submitButton.prop("disabled", true);
    }
  });

  $projectQuestionContent.on("change", "input.ls-checkbox", validateProjectSteps);

  $fileInput.fileupload({
    fileInput:  $fileInput,
    url:        s3PostURL,
    type:       "POST",
    autoUpload: true,
    dataType:   "XML",  // S3 returns XML when success_action_status is set to 201
    replaceFileInput: false,
    add: function(_, data) {
      showSpinner();
      $submitButton.prop("disabled", true);

      var filename = data.files[0].name,
          filesize = data.files[0].size;

      if (filename) { $hiddenFileName.val(filename); }

      $.ajax({
        url:  "/api/s3_credentials",
        type: "GET",
        data: {
          filename: filename,
          filesize: filesize,
          project_token: projectToken
        },
        success: function(s3CreditData) {
          data.formData = s3CreditData;
          data.submit();
        },
        error: function(jqXHR) {
          var errors = $.parseJSON(jqXHR.responseText).error_messages;
          hideSpinner();
          if (jqXHR.status === 422) {
            $currentFile.html(errorMessageBase + " " + errors);
          };
        }
      });
    },
    done: function(_, s3ResponseData) {
      var url      = $(s3ResponseData.jqXHR.responseXML).find("Location").text(),
          filename = $hiddenFileName.val();

      if ($currentFile.length !== 0) {
        $currentFile.empty();
        $currentFile.text(filename);
        $currentFile.append("<i class='fa fa-remove remove-current-file' aria-hidden='true' title='Remove Current File'></i>");
      };

      if ($submitButton.val() === "Post Comment") {
        $submitButton.val("Resubmit Project");
        $previewSubmitButton.text("Resubmit Project");
      };
      $hiddenFileUrl.val(url);
    },
    fail: function(_, data) {
      var status = data.jqXHR.status;

      if (status >= 400) {
        $currentFile.html(errorMessageBase + " There was an error in uploading of your file. Please try again.");
      };
    },
    always: function() {
      $submitButton.prop("disabled", false);
      hideSpinner();
    }
  });

  disableAllCheckBoxes();
  validateProjectSteps();
});
