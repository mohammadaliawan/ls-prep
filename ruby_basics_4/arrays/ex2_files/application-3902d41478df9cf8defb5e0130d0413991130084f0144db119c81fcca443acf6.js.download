function initializeVideoForElement(id) {
  // If we replace any videojs elements, we need to delete their cached
  // players from videojs so they can be setup again.
  delete videojs.players[id];

  // for each videojs element, we need to activate the video-js-hotkeys plugin
  videojs(id, {
    controls: true,
    fluid: true,
    playbackRates: [0.5, 0.75, 1, 1.25, 1.5, 2],
    plugins: {
      videoJsResolutionSwitcher: {
        default: 'high', // Default resolution [{Number}, 'low', 'high'],
        dynamicLabel: true
      }
    }
  }, function() {
    var $player = $(this.el_),
        player  = this;

    player.on('resolutionchange', function() {
      $('.vjs-resolution-button-label', $player).text(player.currentResolution().label);
    });

    player.on('firstplay', function() {
      $player.trigger('firstplay');
    });

    player.hotkeys({
      alwaysCaptureHotkeys: true,
      enableVolumeScroll: false
    });
  });
}
