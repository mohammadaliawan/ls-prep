(function($, PrereqTokenList, HandlebarsTemplates) {
  const MAX_VISIBLE_ITEMS = 7;
  const PrereqTokensApp = function() {
    const app = {
      addPrereqToken: function(e) {
        e.preventDefault();
        let id = $(e.currentTarget).data("id");
        let prereq = this.prereqChoices.find((choice) => choice.id === id);

        this.prereqTokenList.add(prereq);
        this.renderTokens();

        // Don't use form.reset(), because it holds data in hidden fields
        this.$inputPrereq.trigger("blur").val("");
      },

      bindEvents: function() {
        this.$inputPrereq.on("focus", this.showSearchPrompt.bind(this));
        this.$inputPrereq.on("input", this.handleValueChange.bind(this));
        this.$prereqChoices.on("mousedown", "li", this.addPrereqToken.bind(this));
        this.$prereqTokens.on("click", ".remove-token", this.removePrereqToken.bind(this));
        this.$prereqTokens.on("click", ".prereq-token", this.toggleAndCondition.bind(this));
      },

      escapeRegExp: function(string) {
        // only escaping what is potentially relevant to LS course's name
        return string.replace(/[-^$*+?.|()[\]{}]/g, "\\$&");
      },

      handleValueChange: function(e) {
        let choice = e.currentTarget.value;
        let pattern = new RegExp(this.escapeRegExp(choice), "i");

        let visible = this.prereqChoices.filter(function(choice) {
          return pattern.test(choice.full_name);
        });

        this.renderSearchResults(visible.slice(0, MAX_VISIBLE_ITEMS));
      },

      removePrereqToken: function(e) {
        e.preventDefault();
        e.stopPropagation();

        let $target  = $(e.currentTarget);
        let id       = $target.closest(".prereq-token").data("id");
        let prereqId = $target.data("prereq-id");

        this.prereqTokenList.remove(id, prereqId);
        this.renderTokens();
      },

      renderSearchResults: function(results) {
        let template = HandlebarsTemplates["prerequisites/search_results"]({results: results});
        this.$prereqChoices.html(template);
      },

      renderTokens: function() {
        let tokens = this.prereqTokenList.all();
        let template = HandlebarsTemplates["prerequisites/tokens"]({tokens: tokens});
        this.$prereqTokens.html(template);
      },

      showSearchPrompt: function() {
        this.renderSearchResults([{prompt: true}]);
      },

      setData: function() {
        this.prereqChoices   = $("[data-prereq-choices]").data("prereq-choices");
        this.$prereqChoices  = $("#prereq-choices");
        this.$prereqTokens   = $("#prereq-tokens");
        this.$inputPrereq    = $("#input-prerequisite");
        this.prereqTokenList = PrereqTokenList();
        this.prereqTokenList.init(this.$prereqTokens.data("prereq-courses"));
      },

      toggleAndCondition: function(e) {
        e.preventDefault();
        let $activeToken = this.$prereqTokens.find(".active-and-condition");
        let $currentToken = $(e.currentTarget);
        let activeId  = $activeToken.data("id");
        let currentId = $currentToken.data("id");

        if ($activeToken.length !== 0 && activeId !== currentId) {
          let activeToken = this.prereqTokenList.find(activeId);
          $activeToken.removeClass("active-and-condition");
          activeToken.toggleAnd();
        }

        $currentToken.toggleClass("active-and-condition");
        let currentToken = this.prereqTokenList.find(currentId);
        currentToken.toggleAnd();
      },

      init: function() {
        this.setData();
        this.bindEvents();
        this.renderTokens();
      },
    };

    return app;
  }();

  $(function() {
    if ($("#prereq-edit").length !== 0) {
      PrereqTokensApp.init();
    };
  });
})(jQuery, PrereqTokenList, HandlebarsTemplates);
