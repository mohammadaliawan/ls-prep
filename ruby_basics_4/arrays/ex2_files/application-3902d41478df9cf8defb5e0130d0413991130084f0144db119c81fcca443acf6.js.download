      let activeAndToken = tokens.find(token => token.andCondition);

      if (activeAndToken) {
        activeAndToken.add(prereqData);
      } else {
        lastId += 1;
        let token = new PrereqToken(lastId, prereqData);
        tokens.push(token);
      };
    },

    all: function() {
      return tokens.slice();
    },

    find: function(id) {
      return tokens.find(token => token.id === id);
    },

    remove: function(id, prereqId) {
      let token = this.find(id);
      token.remove(prereqId);

      if (token.isEmpty()) {
        let idx = tokens.indexOf(token);
        tokens.splice(idx, 1);
      };
    },

    init: function(rawPrereqs) {
      rawPrereqs.forEach(this.add.bind(this));
    },
  };

