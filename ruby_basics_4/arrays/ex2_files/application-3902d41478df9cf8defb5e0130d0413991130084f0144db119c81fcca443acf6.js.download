    // Example: Window height is 480px and 10px of the footer is visible --
    //          the sticky element will be set to 455px tall, (assuming the
    //          top spacing remains 15px).
    $('.bookshelf .table-of-contents:not(.side-menu)')
      .css({
        height: Math.min(windowHeightWithoutFooter() - tableOfContentsTopSpacing, $(window).height() - tableOfContentsTopSpacing)
      });
  });
