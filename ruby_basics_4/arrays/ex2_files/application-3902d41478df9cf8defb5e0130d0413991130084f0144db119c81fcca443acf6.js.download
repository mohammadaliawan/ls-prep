$(document).ready(function() {

  // Send text to server and yield the resulting HTML to callback.
  function renderMarkdownOnServer(text, callback) {
    if (text.match(/^\s*$/)) {
      callback("There is nothing to preview.");
      return;
    }
    $.post("/api/markdown", {text: text}, function(data, _, jqXHR) {
      if (jqXHR.status === 200) {
        callback(data);
      } else {
        callback("A preview could not be generated.");
      }
    });
  }

  // Show markdown preview modal and update it with rendered HTML.
  function showMarkdownPreview(title, bodyText, context) {
    var $preview = $('.markdown-preview', context),
        $header  = $('.preview-header', $preview),
        $content = $('.preview-content', $preview);

    if (title) { $header.text(title); }
    $content.html('<p>loading preview...</p>');

    renderMarkdownOnServer(bodyText, function(html) {
      $content.html(html);
      $content.find('code').each(function(_, element) {
        Prism.highlightElement(element);
      });
    });
  }

  // Export function for use outside this module
  window.Markdown = {
    preview: renderMarkdownOnServer,
    showPreview: showMarkdownPreview
  };

});


